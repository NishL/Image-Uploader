<h1>Posts#index</h1>
<p>Find me in app/views/posts/index.html.erb</p>
<br>

<%= link_to "New", new_post_path, class: "btn btn-default" %>
<hr>

<% @posts.map(&:pictures).each do |picture| %>
  <% picture.each do |p| %>
    <%= link_to "#{image_tag p.image.medium}".html_safe, post_path(p.post_id) %>
  <% end %>
<% end %>
